{
  "name": "c-blinken",
  "version": "1.0.0",
  "description": "Animations manager for c-base and c-flo",
  "main": "index.js",
  "scripts": {
    "dev": "noflo-nodejs --graph graphs/Animate.json --secret=c-blinken",
    "lint": "eslint *.js lib/*.js components/*.js",
    "postinstall": "noflo-cache-preheat",
    "start": "noflo-runtime-msgflo --name c-blinken --cache=true --basedir . --graph c-blinken/Animate",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/c-base/c-blinken.git"
  },
  "keywords": [
    "noflo",
    "iot",
    "animation"
  ],
  "author": "Henri Bergius <henri.bergius@iki.fi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/c-base/c-blinken/issues"
  },
  "homepage": "https://github.com/c-base/c-blinken#readme",
  "dependencies": {
    "color": "^3.1.0",
    "noflo": "^1.1.3",
    "noflo-core": "^0.6.0",
    "noflo-runtime-msgflo": "^0.12.0"
  },
  "devDependencies": {
    "eslint": "^5.11.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "noflo-nodejs": "^0.10.1"
  }
}
